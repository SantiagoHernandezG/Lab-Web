extends layout

block content
    .container-contact100
        .wrap-contact200
            span.contact100-form-title
                | Dashboard
            table.table
                thead
                    tr
                        th(scope='col') Código
                        th(scope='col') Nombres
                        th(scope='col') Apellidos
                        th(scope='col') Mail
                        th(scope='col') Teléfono
                        th(scope='col') Ticket
                tbody
                    each user in users
                        tr
                            th(scope='row')=user.code
                            td=user.names
                            td=user.last_names
                            td=user.mail
                            td=user.phone
                            td=user.ticket
            .container-contact100-form-btn
                .wrap-contact100-form-btn
                    .contact100-form-bgbtn
                    button.contact100-form-btn
                        span
                            a(href='/') Home
                            i.fa.fa-long-arrow-right.m-l-7(aria-hidden='true')